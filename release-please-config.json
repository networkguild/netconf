{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "go",
      "versioning": "default",
      "include-v-in-tag": true,
      "changelog-sections": [
        {
          "type": "feat",
          "hidden": false,
          "section": "Features"
        },
        {
          "type": "feature",
          "hidden": false,
          "section": "Features"
        },
        {
          "type": "fix",
          "hidden": false,
          "section": "Bug Fixes"
        },
        {
          "type": "chore",
          "hidden": false,
          "section": "Miscellaneous Chores"
        },
        {
          "type": "deps",
          "hidden": false,
          "section": "Dependencies"
        }
      ]
    }
  }
}
